                    <row rowsep="1" class="- topic/row " xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/references/security.dita" xtrc="row:1;139:37">
                        <entry colsep="1" class="- topic/entry " xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/references/security.dita" xtrc="entry:1;140:43" colname="col1" dita-ot:x="1" dita-ot:y="1">Account</entry>
                        <entry colsep="1" class="- topic/entry " xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/references/security.dita" xtrc="entry:2;141:43" colname="col2" dita-ot:x="2" dita-ot:y="1">Default password </entry>
                       
                        <entry colsep="1" class="- topic/entry " xtrf="file:/C:/Users/patscully/Dropbox/Current%20Working%20Clients/Information%20Designs%20&amp;%20Development/project_2_ps/workspace/publications/deployment_and_configuration_guide/topics/references/security.dita" xtrc="entry:3;143:43" colname="col3" dita-ot:x="3" dita-ot:y="1">Description </entry>
                        
                    </row>
                </thead>
